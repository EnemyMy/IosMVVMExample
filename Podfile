# platform :ios, '9.0'

target 'MVVMFirst' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  pod "EasyPeasy"
  pod "RxSwift"
  pod "RxRelay"
  pod "RxCocoa"

  # Pods for MVVMFirst

  target 'MVVMFirstTests' do
    inherit! :search_paths
    # Pods for testing
    pod "EasyPeasy"
    pod "RxSwift"
    pod "RxRelay"
    pod "RxCocoa"
  end

  target 'MVVMFirstUITests' do
    # Pods for testing
    pod "EasyPeasy"
    pod "RxSwift"
    pod "RxRelay"
    pod "RxCocoa"
  end

end
